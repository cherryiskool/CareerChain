<div class="editProfileFormAndX" id="editProfileForm">
    <button class="formExit" hx-delete="/editProfileForm"
            hx-target="#editProfileForm"
            hx-swap="outerHTML">X</button>
    <form class="editProfileForm" onsubmit="return deployContract(event)" action="/editProfileContractAddress" method="POST" enctype="application/x-www-form-urlencoded">
        <div id="editProfileFormEducation">

        </div>
        <button class="addInputButton"
                hx-get="/editProfileForm/EducationInput"
                hx-target="#editProfileFormEducation"
                hx-swap="beforeend">Add Education</button>

        <div id="editProfileFormWorkHistory">

        </div>
        <button class="addInputButton"
                hx-get="/editProfileForm/WorkInput"
                hx-target="#editProfileFormWorkHistory"
                hx-swap="beforeend">Add Work History</button>
        <div></div>
        <button class="addInputButton" type="submit">Save to BlockChain</button>
    </form>
</div>